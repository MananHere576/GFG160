class Solution {
    public static Node findFirstNode(Node head) {
        // code here
        HashSet<Node>arr=new HashSet<>();
        Node temp=head;
        while(temp!=null){
            if(arr.contains(temp)){
                return temp;
            }
            else
        {
            arr.add(temp);
            
        }
        temp=temp.next;
        }
        
        return new Node(-1);
    }
}
